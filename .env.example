# SearXNG MCP Server Configuration
# Copy this file to .env and customize as needed

# Base URL of your SearXNG instance
# Default: http://localhost:8080
# Note: If using the provided docker-compose.yaml, use http://localhost:9999
SEARXNG_BASE_URL=http://localhost:9999

# Request timeout in seconds
# Default: 10
SEARXNG_TIMEOUT=10

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# Default: INFO
LOG_LEVEL=INFO

# Maximum number of results allowed per query
# Default: 50
# Maximum: 100
MAX_RESULTS_LIMIT=50
